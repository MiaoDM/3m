.page {
  display: block;
  min-height: 100%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}

.logo {
  position: fixed;
  left: 10rpx;
  top: 40rpx;
  z-index: 10001;
}

.img_logo {
  width: 450rpx;
  height: 60rpx;
}

.title {
  margin-top: 20%;
  height: 40%;
  font-size: 40rpx;
  text-align: left;
  margin-left:5%;
  margin-right:5%;
  color: #FFF;
}

.text {
  margin-top: 5%;
  height: 40%;
  margin-left:5%;
  margin-right:5%;
  font-size: 32rpx;
  text-align: left;
  color: #FFF;
}

.button{
  height: 60rpx;
  position: fixed; 
  width:200px; 
  height:35px;
  margin: auto;
  left:0; right:0;
  border-radius: 10px;
  text-align: center;
  font-size: 32rpx;
  color: #FFF;
  bottom: 50rpx;
  background-color: #333; 
}

